{
  "manifest_version": 3,
  "name": "Procore Search Shrinker",
  "description": "It shrinks the Procore search box to give you more room for your favorites.",
  "version": "1.2",
  "action": {
    "default_popup": "search-shrinker.html",
    "default_icon": "shrinker.png"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "matches": ["https://sandbox.procore.com/*", "https://procore.com/*", "https://app.procore.com/*"]
    }
  ]
}
